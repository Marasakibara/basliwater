{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "public",
        "includeFiles": [
          "public/**"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "headers": {
        "Cache-Control": "s-maxage=31536000, immutable"
      }
    }
  ]
}